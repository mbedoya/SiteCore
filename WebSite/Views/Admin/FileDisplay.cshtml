@model WebSite.Models.FileUIModel

@if (Model.Type == WebSite.Models.FileUIModel.FileType.Image)
{
    <a class="fancybox" href="@Model.URL">
        <img style="width:50px;height:50px" src="@Model.URL" />
    </a>    
}
else
{
    if (Model.Type == WebSite.Models.FileUIModel.FileType.File)
    {
        <a href="@Model.URL" target="_blank">
            <img style="width:50px;height:50px" src="/content/img/attachment.png" />
        </a>
    }    
}